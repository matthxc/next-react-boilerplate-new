{
  "name": "react-boilerplate-next",
  "version": "2.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=12.14.0"
  },
  "author": "Juan Felipe Camargo",
  "license": "MIT",

  "scripts": {
    "start:dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals --fix",
    "lint:js": "npm run lint:eslint -- . --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export",
    "cli": "pankod-cli add",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.4.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.8.5",
    "bootstrap": "^4.5.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.4.4",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^4.5.0",
    "next-redux-wrapper": "^6.0.2",
    "next-routes": "^1.4.2",
    "next-runtime-dotenv": "^1.2.0",
    "node-sass": "^4.14.1",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-redux": "^7.1.9",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.1",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "10.x",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-ts": "^0.4.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "5.x",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.1.4",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "fork-ts-checker-webpack-plugin": "^5.0.7",
    "husky": "4.2.5",
    "jest": "^26.1.0",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "10.2.11",
    "nock": "^13.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "^26.1.2",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "3.8.3"
  }
}
