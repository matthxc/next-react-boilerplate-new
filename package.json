{
  "name": "next-react-boilerplate",
  "version": "1.0.0",
  "description": "Highly scalable and basic Next JS boilerplate with Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthxc/next-react-boilerplate-new.git"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=12.14.0"
  },
  "author": "Juan Felipe Camargo",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "build": "rimraf ./.next && next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "test:e2e:run:novideo": "cypress run --config video=false",
    "test:e2e:run": "rimraf __tests__/integration/e2e/videos && cypress run",
    "test:e2e:open": "cypress open",
    "test:perf": "rimraf .lighthouseci && lhci autorun",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals",
    "lint:js": "npm run lint:eslint -- . --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "export": "cross-env NODE_ENV=production next export"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.2",
    "@next/bundle-analyzer": "^10.0.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^6.1.1",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-runtime-dotenv": "^1.3.0",
    "polished": "^4.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx-source": "^7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@lhci/cli": "^0.6.1",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-eslint": "10.x",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cypress": "^6.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-ts": "^0.4.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "fork-ts-checker-webpack-plugin": "^6.0.7",
    "husky": "4.3.6",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "10.5.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "13.8.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "4.1.3"
  }
}
